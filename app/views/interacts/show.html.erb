<div class="page-header">
  <h3>Interaction</h3>
</div>
<div class="row">
  <div class="col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><%= @interact.ititle %></h3>
      </div>
      <div class="panel-body">
        <dl class="dl-horizontal">
          <dt>Body</dt>
          <dd><%= @interact.ibody %></dd>

          <dt>Date</dt>
          <dd><%= @interact.idate %></dd>

          <dt>Contact</dt>
          <dd><a href="/contacts/<%= Contact.find(@interact.contact_id).id %>">
            <%= Contact.find(@interact.contact_id).cfname %> <%= Contact.find(@interact.contact_id).clname %>
            </a>
          </dd>

        </dl>

        <div class="btn-group navbar-right">
          <a href="/interacts/<%= @interact.id %>/edit" class="btn btn-default btn-sm">
            <i class="fa fa-edit fa-fw"></i>
          </a>
          <a href="/delete_interact/<%= @interact.id %>" class="btn btn-default btn-sm" rel="nofollow">
            <i class="fa fa-trash-o fa-fw"></i>
          </a>
        </div>
      </div>
    </div>
  </div>
</div>
