<div class="page-header">
  <h1>
    Contacts
    <a href="/contacts/new" class="btn btn-lg btn-success">New Contact</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Cfname</th>
        <th>Clname</th>
        <th>Cemail</th>
        <th>Cdob</th>
        <th>Caddress</th>
        <th>Cphone</th>
        <th>Ccompany</th>
        <th>Cindustry</th>
        <th>Cinterest</th>
        <th>Crelation</th>
        <th>Cnote</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.cfname %></td>
        <td><%= contact.clname %></td>
        <td><%= contact.cemail %></td>
        <td><%= contact.cdob %></td>
        <td><%= contact.caddress %></td>
        <td><%= contact.cphone %></td>
        <td><%= contact.ccompany %></td>
        <td><%= contact.cindustry %></td>
        <td><%= contact.cinterest %></td>
        <td><%= contact.crelation %></td>
        <td><%= contact.cnote %></td>
        <td><%= contact.user_id %></td>
        <td>
          <a href="/contacts/<%= contact.id %>" class="btn btn-primary">Show</a>
          <a href="/contacts/<%= contact.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_contact/<%= contact.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
